<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Assets\Images\boo.png" />
    <None Remove="Assets\Images\clears.png" />
    <None Remove="Assets\Images\clears_blue.png" />
    <None Remove="Assets\Images\clears_white.png" />
    <None Remove="Assets\Images\deaths.png" />
    <None Remove="Assets\Images\like.png" />
